{
  "expo": {
    "name": "khabarx",
    "slug": "khabarx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "khabarx",
    "userInterfaceStyle": "automatic",
  "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.amoghnya.khabarx",
      "googleServicesFile": "./google-services.json",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-notifications",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow khabarx to access your photos to attach media to your posts.",
          "cameraPermission": "Allow khabarx to use your camera to capture photos and videos.",
          "microphonePermission": "Allow khabarx to record audio with videos."
        }
      ],
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "google": {
        "androidClientId": "__AUTO_FROM_GOOGLE_SERVICES_JSON__",
        "iosClientId": "YOUR_IOS_CLIENT_ID.apps.googleusercontent.com",
        "webClientId": "572758255210-gubenuc6g6p9j5tudgaqo5m6lc8irtn4.apps.googleusercontent.com",
        "mode": "native"
      },
      "firebase": {
        "webApiKey": "AIzaSyAIwciUGr4JgSo7keO0TiSPzS3E70SeUyw",
        "projectId": "khabarx-f0365",
  "appId": "1:572758255210:android:4ee81e7bc477d728f44c13",
        "messagingSenderId": "572758255210",
        "storageBucket": "khabarx-f0365.firebasestorage.app"
      },
      "router": {},
      "eas": {
        "projectId": "6f9f8ff8-f44f-4164-b5eb-591e984e732a"
      }
    },
    "experiments": {
      "typedRoutes": true
    }
  }
}
