    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "clean:win": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"Remove-Item -Recurse -Force node_modules; Remove-Item -Force package-lock.json -ErrorAction SilentlyContinue; Remove-Item -Recurse -Force .\\node_modules\\.cache\\* -ErrorAction SilentlyContinue; Remove-Item -Recurse -Force $env:TEMP\\metro-cache-* -ErrorAction SilentlyContinue; Remove-Item -Recurse -Force $env:TEMP\\haste-map-* -ErrorAction SilentlyContinue; echo 'Clean complete. Run npm install and then npx expo run:android';\"",
    "android:apk:release": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/use-jdk17.ps1 -- ./android/gradlew.bat assembleRelease --no-daemon --stacktrace --console=plain",
    "android:bundle:release": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/use-jdk17.ps1 -- ./android/gradlew.bat bundleRelease --no-daemon --stacktrace --console=plain"
  },
